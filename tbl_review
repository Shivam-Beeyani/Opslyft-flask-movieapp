CREATE TABLE `tbl_rev` (
  `rev_id` int(11) NOT NULL AUTO_INCREMENT,
  `rev_title` varchar(45) DEFAULT NULL,
  `rev_rate` tinyint DEFAULT NULL,
  `rev_description` varchar(5000) DEFAULT NULL, 
  `rev_user_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`rev_id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=latin1;